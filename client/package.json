{
  "name": "client",
  "version": "0.1.1",
  "scripts": {
    "ng": "npx ng",
    "dev": "npx ng serve --port 4220 -o",
    "dev:prod": "npx ng serve --port 4220 -o --prod",
    "dev:ssl": "npx ng serve --port 4220 -o --ssl --ssl-key $HTTP_TLS_KEY --ssl-cert $HTTP_TLS_CERTIFICATE",
    "dev:prod:ssl": "npx ng serve --port 4220 -o --prod --ssl --ssl-key $HTTP_TLS_KEY --ssl-cert $HTTP_TLS_CERTIFICATE",
    "dev:ssl:prod": "npm run dev:prod:ssl",
    "start": "npx ng serve",
    "build": "npx ng build",
    "build:prod": "npx ng build --prod",
    "test": "npx ng test",
    "lint": "npx ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.4",
    "@angular/cdk": "6.2.1",
    "@angular/common": "6.1.4",
    "@angular/compiler": "6.1.4",
    "@angular/core": "6.1.4",
    "@angular/flex-layout": "^6.0.0-beta.17",
    "@angular/forms": "6.1.4",
    "@angular/http": "6.1.4",
    "@angular/material": "6.2.1",
    "@angular/platform-browser": "6.1.4",
    "@angular/platform-browser-dynamic": "6.1.4",
    "@angular/router": "6.1.4",
    "core-js": "2.5.6",
    "deep-copy": "1.4.2",
    "deep-object-diff": "1.1.0",
    "loglevel": "^1.6.1",
    "rxjs": "6.2.2",
    "traverse": "0.6.6",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "6.0.4",
    "@angular/language-service": "6.0.4",
    "@types/loglevel": "^1.5.3",
    "@types/node": "~8.9.4",
    "@types/traverse": "^0.6.32",
    "codelyzer": "~4.2.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
